{
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "import/order": [
    "error",
    {
      "groups": [
        "index",
        "sibling",
        "parent",
        "internal",
        "external",
        "builtin",
        "object",
        "type"
      ]
    }
  ],
  "plugins": ["react", "react-hooks", "jsx-a11y"],
  "rules": {
    "strict": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
